{
  "connection": "default",
  "collectionName": "post",
  "info": {
    "name": "post",
    "description": ""
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "title": {
      "required": true,
      "type": "string"
    },
    "author": {
      "model": "user",
      "plugin": "users-permissions"
    },
    "content": {
      "collection": "block",
      "via": "posts"
    },
    "summary": {
      "required": true,
      "type": "text"
    },
    "dek": {
      "type": "string"
    },
    "location": {
      "required": true,
      "type": "string"
    },
    "slug": {
      "required": true,
      "type": "string",
      "unique": true
    },
    "tags": {
      "collection": "tag",
      "via": "posts"
    },
    "published": {
      "required": true,
      "type": "boolean",
      "default": true
    },
    "updated": {
      "required": true,
      "type": "date"
    },
    "assets": {
      "collection": "asset",
      "via": "posts",
      "dominant": true
    },
    "collections": {
      "collection": "collection",
      "via": "posts"
    }
  }
}